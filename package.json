{
  "name": "ap-npm",
  "version": "0.5.2",
  "description": "Private Authenticated NPM Repository",
  "main": "lib/index.js",
  "scripts": {
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "start": "npm run serve",
    "serve": "node lib/cli.js s",
    "serve-ts": "ts-node src/cli.ts serve",
    "lint": "eslint src/**/*.ts && tslint src/**/*.ts",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "mocha",
    "test-coverage": "nyc npm test",
    "test-report": "nyc report",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "bin": {
    "ap-npm": "./bin/ap-npm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genkgo/ap-npm.git"
  },
  "author": "Frederik Bosh, MÃ¨ir Noordermeer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/genkgo/ap-npm/issues"
  },
  "homepage": "https://github.com/genkgo/ap-npm#readme",
  "dependencies": {
    "@contentful/axios": "^0.18.0",
    "async-file": "^2.0.2",
    "body-parser": "^1.18.3",
    "colors": "^1.3.0",
    "commander": "^2.16.0",
    "express": "^4.15.2",
    "http-proxy": "^1.17.0",
    "js-sha512": "^0.7.1",
    "mssql": "^4.1.0",
    "mysql": "^2.15.0",
    "pg": "^7.4.3",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "semver": "^5.5.0",
    "sqlite3": "^4.0.2",
    "ts-md5": "^1.2.4"
  },
  "keywords": [
    "private",
    "npm",
    "package",
    "repository",
    "registry",
    "server"
  ],
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.4",
    "@types/colors": "^1.2.1",
    "@types/express": "^4.16.0",
    "@types/js-sha512": "^0.7.0",
    "@types/mocha": "^5.2.5",
    "@types/nock": "^9.3.0",
    "@types/node": "^10.5.2",
    "@types/rimraf": "^2.0.2",
    "@types/semver": "^5.5.0",
    "@types/sinon-chai": "^3.2.0",
    "chai": "^4.1.2",
    "codecov": "^3.0.4",
    "coveralls": "^3.0.2",
    "md5": "^2.2.1",
    "mocha": "^5.2.0",
    "nock": "^9.4.2",
    "node-mocks-http": "^1.7.0",
    "nyc": "^12.0.2",
    "prettier": "^1.13.7",
    "sinon": "^6.1.3",
    "sinon-chai": "^3.2.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.13.0",
    "typemoq": "^2.1.0",
    "typescript": "^2.9.2"
  },
  "nyc": {
    "check-coverage": true,
    "extension": [
      ".js",
      ".jsx",
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/!(*.test.*).[tj]s?(x)"
    ],
    "exclude": [
      "**/*.d.ts",
      "lib/**/*",
      "src/storage/storage-provider.ts",
      "src/storage/filesystem/fs-interface.ts",
      "src/storage/filesystem/index.ts",
      "src/cli.ts",
      "src/init.ts",
      "src/index.ts",
      "src/routes.ts",
      "src/commands/*"
    ],
    "reporter": [
      "json",
      "text"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "all": true
  }
}
